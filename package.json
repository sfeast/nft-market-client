    "@mui/styled-engine-sc": "^5.5.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "buffer": "^6.0.3",
    "casper-cep47-js-client": "^1.0.1",
    "casper-js-client-helper": "^1.0.1",
    "casper-js-sdk": "^2.9.1",
    "classnames": "^2.3.1",
    "final-form": "^4.20.6",
    "firebase": "^9.6.9",
    "ipfs-core": "^0.14.2",
